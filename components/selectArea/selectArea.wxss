/* components/selectArea/selectArea.wxss */
.popbox{
  display:flex;
  flex-direction: column;
  flex-wrap: wrap;
  margin:20rpx;
  height:300rpx;
  justify-content: space-around;
}
.selected{
  height:70rpx;
  width:400rpx;
  border:1rpx solid gray;
  border-radius: 20rpx;
  display:flex;
  align-items: center;
  padding-left: 10rpx;
}
.down{
  height:50rpx;
  width:50rpx;
  margin-left:auto;
}


