
.areaTitle{
  background-color: rgb(181, 234, 255);
  width:100%;
  height:100rpx;
  line-height: 100rpx;
  font-weight: bold;
  text-align: center;
  margin-top:20rpx;
}
.box{
  display: flex;
  flex-wrap:wrap;
  /* white-space: nowrap; */
}
.areaItem{
  width:33.33%;
  height:200rpx;
  display:flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  border:10rpx solid white;
  background-color: rgb(201, 201, 201);
  box-sizing: border-box;
  border-radius: 20px;
}